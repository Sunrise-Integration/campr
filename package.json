{
  "name": "campr",
  "version": "1.0.0",
  "description": "A CLI for github's graphql api",
  "main": "index.js",
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=./webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=./webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=./webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "campr": "./index.js"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Sunrise-Integration/github-graphql.git"
  },
  "author": "Matthew McKey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sunrise-Integration/github-graphql/issues"
  },
  "homepage": "https://github.com/Sunrise-Integration/github-graphql#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^6.1.0",
    "require": "^2.4.20",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "eslint": "^5.8.0"
  }
}
